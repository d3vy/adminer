{
	"name": "d3vy/adminer",
	"license": "WTFPL",
	"type": "project",
	"description": "A small frame for Adminer to use it with composer",
	"homepage": "https://github.com/d3vy/adminer",
	"authors": [
		{
			"name": "Szabolcs Sur√°nyi",
			"email": "szabolcs.suranyi@shadesoft.hu",
			"homepage": "https://shadesoft.dev"
		}
	],
	"require": {
		"php": "^5.5.9|^7",
		"archeloth/adminer-theme": "*",
		"symfony/yaml": "^3.4|^4|^5",
		"vrana/adminer": "*"
	},
	"config": {
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"": "src"
		}
	},
	"scripts": {
		"post-install-cmd": [
			"@php -f vendor/vrana/adminer/compile.php"
		],
		"post-update-cmd": [
			"@php -f vendor/vrana/adminer/compile.php"
		]
	}
}
