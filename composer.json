{
	"name": "subbysnake/adminer",
	"license": "MIT",
	"type": "project",
	"description": "A small frame for Adminer to use it with composer",
	"authors": [
		{
			"name": "Szabolcs Sur√°nyi",
			"email": "szabolcs.suranyi@shadesoft.hu",
			"homepage": "http://shadesoft.hu",
			"role": "developer"
		}
	],
	"require": {
		"php": "^5.5.9",
		"symfony/yaml": "^3.4",
		"vrana/adminer": "^4.6.3"
	},
	"config": {
		"platform": {
			"php": "5.5.9"
		},
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"Command\\": "command/"
		}
	},
	"scripts": {
		"post-install-cmd": [
			"php -f vendor/vrana/adminer/compile.php",
			"Command\\Config::generate"
		],
		"post-update-cmd": [
			"php -f vendor/vrana/adminer/compile.php",
			"Command\\Config::generate"
		]
	}
}
