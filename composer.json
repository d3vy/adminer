	"type": "project",
	"description": "A small frame for Adminer to use it with composer",
	"homepage": "https://github.com/d3vy/adminer",
	"authors": [
		{
			"name": "Szabolcs SurÃ¡nyi",
			"email": "szabolcs.suranyi@shadesoft.hu",
			"homepage": "http://shadesoft.hu",
			"role": "developer"
		}
	],
	"require": {
		"php": ">=5.5.9",
		"symfony/yaml": "^3.4|^4|^5",
		"vrana/adminer": "*"
	},
	"config": {
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"Command\\": "command/"
		}
	},
	"scripts": {
		"post-install-cmd": [
			"php -f vendor/vrana/adminer/compile.php",
			"Command\\Config::generate"
		],
		"post-update-cmd": [
			"php -f vendor/vrana/adminer/compile.php",
			"Command\\Config::generate"
		]
	}
}
